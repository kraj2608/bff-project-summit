{
  "name": "bff-foodshop",
  "version": "0.0.0",
  "description": "BFF for food shop micro services",
  "main": "dist",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "build": "rimraf dist && babel src --out-dir dist  --copy-files",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kraj2608/bff-project-summit.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.2",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.13.3",
    "http-status-codes": "^2.2.0",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "swagger-ui-express": "^4.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "nodemon": "^2.0.20",
    "rimraf": "^4.1.2"
  },
  "bugs": {
    "url": "https://github.com/kraj2608/bff-project-summit/issues"
  },
  "homepage": "https://github.com/kraj2608/bff-project-summit#readme"
}
